<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_checked="true" android:state_enabled="false">
        <shape android:shape="rectangle">
            <solid android:color="#b3ecbc" />
            <corners android:radius="20dp" />
        </shape>
    </item>
    <item android:state_checked="false" android:state_enabled="false">
        <shape android:shape="rectangle">
            <solid android:color="#FEFEFE" />
            <corners android:radius="20dp" />
            <stroke android:width="1.5dp" android:color="#F0F0F0" />
        </shape>
    </item>
    <item android:state_checked="true">
        <shape android:shape="rectangle">
            <solid android:color="#43d95d" />
            <corners android:radius="20dp" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <solid android:color="#FEFEFE" />
            <corners android:radius="20dp" />
            <stroke android:width="1.5dp" android:color="#E6E6E6" />
        </shape>
    </item>
</selector>