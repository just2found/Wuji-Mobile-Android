<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/safe_box_list_nav"
    app:startDestination="@id/quickCloudNavFragment">

    <fragment
        android:id="@+id/quickCloudNavFragment"
        android:name="net.linkmate.app.ui.nas.safe_box.list.SafeBoxQuickCloudNavFragment"
        android:label="VP2QuickCloudNavFragment"

        tools:layout="@layout/fragment_safe_box_nas_file">
        <argument
            android:name="deviceid"
            app:argType="string" />

        <action
            android:id="@+id/action_list_to_move_in"
            app:destination="@id/moveInFragment"
            app:enterAnim="@anim/slide_right_in"
            app:exitAnim="@anim/slide_left_out"
            app:popEnterAnim="@anim/slide_left_in"
            app:popExitAnim="@anim/slide_right_out" />

        <action
            android:id="@+id/action_list_to_transfer"
            app:destination="@id/transferFragment"
            app:enterAnim="@anim/slide_right_in"
            app:exitAnim="@anim/slide_left_out"
            app:popEnterAnim="@anim/slide_left_in"
            app:popExitAnim="@anim/slide_right_out" />

        <action
            android:id="@+id/action_list_to_move_to"
            app:destination="@id/moveToFragment"
            app:enterAnim="@anim/slide_right_in"
            app:exitAnim="@anim/slide_left_out"
            app:popEnterAnim="@anim/slide_left_in"
            app:popExitAnim="@anim/slide_right_out" />
        <action
            android:id="@+id/action_list_to_upload"
            app:destination="@id/uploadNavFragment"
            app:enterAnim="@anim/slide_right_in"
            app:exitAnim="@anim/slide_left_out"
            app:popEnterAnim="@anim/slide_left_in"
            app:popExitAnim="@anim/slide_right_out" />
    </fragment>


    <fragment
        android:id="@+id/moveInFragment"
        android:name="net.linkmate.app.ui.nas.safe_box.list.SafeBoxMoveInFragment"
        android:label="SafeBoxMoveInFragment"
        tools:layout="@layout/fragment_choice_file1">
        <argument
            android:name="deviceid"
            app:argType="string" />
        <argument
            android:name="path"
            app:argType="string" />

        <action
            android:id="@+id/action_in_to_set_path"
            app:destination="@id/safeBoxSetPathFragment"
            app:enterAnim="@anim/slide_right_in"
            app:exitAnim="@anim/slide_left_out"
            app:popEnterAnim="@anim/slide_left_in"
            app:popExitAnim="@anim/slide_right_out" />
    </fragment>

    <fragment
        android:id="@+id/transferFragment"
        android:name="net.linkmate.app.ui.nas.safe_box.transfer.SbTransferFragment"
        android:label="SbTransferFragment"
        tools:layout="@layout/fragment_nas_transfer">
        <argument
            android:name="deviceid"
            app:argType="string" />
    </fragment>

    <fragment
        android:id="@+id/moveToFragment"
        android:name="net.linkmate.app.ui.nas.safe_box.list.SafeBoxMoveToFragment"
        android:label="SafeBoxMoveToFragment"
        tools:layout="@layout/fragment_choice_nas">
        <argument
            android:name="deviceid"
            app:argType="string" />
        <argument
            android:name="type"
            app:argType="string" />
    </fragment>

    <fragment
        android:id="@+id/safeBoxSetPathFragment"
        android:name="net.linkmate.app.ui.nas.safe_box.list.SafeBoxSetPathFragment"
        android:label="SafeBoxSetPathFragment"
        tools:layout="@layout/fragment_choice_nas">
        <argument
            android:name="deviceid"
            app:argType="string" />
        <argument
            android:name="share_type"
            app:argType="integer" />
        <argument
            android:name="paths"
            app:argType="string[]" />

        <action
            android:id="@+id/action_set_to_index"
            app:destination="@id/quickCloudNavFragment"
            app:enterAnim="@anim/slide_right_in"
            app:exitAnim="@anim/slide_left_out"
            app:popEnterAnim="@anim/slide_left_in"
            app:popExitAnim="@anim/slide_right_out" />
    </fragment>



    <fragment
        android:id="@+id/uploadNavFragment"
        android:name="net.linkmate.app.ui.nas.safe_box.list.SafeBoxUploadNavFragment"
        android:label="SafeBoxUploadNavFragment"
        tools:layout="@layout/fragment_nas_upload">
        <argument
            android:name="deviceid"
            app:argType="string" />
        <argument
            android:name="path"
            app:argType="string" />
<!--        <argument-->
<!--            android:name="fileType"-->
<!--            app:argType="net.sdvn.nascommon.model.phone.LocalFileType" />-->

        <action
            android:id="@+id/action_upload_to_set_path"
            app:destination="@id/uploadSetPathFragment"
            app:enterAnim="@anim/slide_right_in"
            app:exitAnim="@anim/slide_left_out"
            app:popEnterAnim="@anim/slide_left_in"
            app:popExitAnim="@anim/slide_right_out" />
    </fragment>


    <fragment
        android:id="@+id/uploadSetPathFragment"
        android:name="net.linkmate.app.ui.nas.safe_box.list.UploadSetPathFragment"
        android:label="SafeBoxUploadNavFragment"
        tools:layout="@layout/fragment_choice_nas">
        <argument
            android:name="deviceid"
            app:argType="string" />
        <action
            android:id="@+id/action_upload_to_index"
            app:destination="@id/quickCloudNavFragment"
            app:enterAnim="@anim/slide_right_in"
            app:exitAnim="@anim/slide_left_out"
            app:popEnterAnim="@anim/slide_left_in"
            app:popExitAnim="@anim/slide_right_out"
            app:popUpTo="@id/uploadSetPathFragment"
            app:popUpToInclusive="true"/>
    </fragment>


    <include app:graph="@navigation/files_nav_search" />
    <action
        android:id="@+id/action_global_searchFragment"
        app:destination="@id/files_nav_search"
        app:popUpTo="@id/files_nav_search"
        app:popUpToInclusive="true" />

</navigation>