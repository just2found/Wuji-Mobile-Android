<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/files_nav_search"
    app:startDestination="@id/searchFragment">


    <fragment
        android:id="@+id/searchFragment"
        android:name="net.linkmate.app.ui.nas.search.SearchFragment"
        android:label="ImageFragment">
        <argument
            android:name="deviceid"
            app:argType="string" />
        <argument
            android:name="fileType"
            android:defaultValue="ALL"
            app:argType="net.sdvn.nascommon.model.oneos.OneOSFileType" />
        <argument
            android:name="pathType"
            app:argType="integer[]" />
        <argument
            android:name="curPath"
            android:defaultValue="@null"
            app:argType="string"
            app:nullable="true" />

        <argument
            android:name="groupid"
            android:defaultValue="-1L"
            app:argType="long" />
    </fragment>

    <fragment
        android:id="@+id/searchImagePreviewFragment"
        android:name="net.linkmate.app.ui.nas.search.SearchImagePreviewFragment"
        android:label="ImageFragment" />

    <action
        android:id="@+id/action_global_searchImagePreviewFragment"
        app:destination="@id/searchImagePreviewFragment"
        app:popUpTo="@id/searchImagePreviewFragment"
        app:popUpToInclusive="true" />



</navigation>